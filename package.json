{
	"name": "vite-plugin-mdicon2svg",
	"description": "Vite plugin to transform md-icon to svgs",
	"version": "0.2.3",
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"material-3",
		"material-web-components",
		"mwc",
		"optimization"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vdegenne/vite-plugin-mdicon2svg"
	},
	"files": [
		"./lib/"
	],
	"devDependencies": {
		"@changesets/cli": "^2.29.2",
		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/node": "^24.5.2",
		"rollup": "^4.40.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.3",
		"wireit": "^0.14.12"
	},
	"dependencies": {
		"@rollup/plugin-virtual": "^3.0.2",
		"@rollup/pluginutils": "^5.1.4",
		"fast-glob": "^3.3.3",
		"lit-html": "^3.0.0",
		"mwc3-back-helpers": "^1.0.3"
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"dependencies": [
				"build:ts"
			]
		},
		"build:ts": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"./tsconfig.json",
				"./src/"
			],
			"output": [
				"./lib"
			]
		},
		"build:rollup": {
			"command": "rollup -c",
			"dependencies": [
				"build:ts"
			],
			"files": [
				"./rollup.config.js"
			],
			"output": [
				"./index.js"
			]
		}
	}
}
